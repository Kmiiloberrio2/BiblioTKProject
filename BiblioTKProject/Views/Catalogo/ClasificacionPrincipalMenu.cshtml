@model IEnumerable<BiblioTK.Modelos.ClasficacionPrincipalResult>


<ul  id="clasficacionDiv" class="nav nav-stacked affix">
    <li class="active"><a href="#dropdowns"><i class="icon-chevron-right"></i> Categorias</a></li>
    @foreach (var item in Model)
    {
        <li class=""><a href="#Clasificacion"><span class="badge badge-warning">@item.Items</span> @item.ClasificacionPrincipal</a></li>
    }
</ul>

